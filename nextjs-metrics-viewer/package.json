{
  "name": "flowbite-react-template-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd ws-server && npm install",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:ws\"",
    "dev:next": "next dev",
    "dev:ws": "cd ws-server && npm run dev",
    "build": "next build && cd ws-server && npm run build",
    "start": "concurrently \"npm run start:next\" \"npm run start:ws\"",
    "start:next": "next start",
    "start:ws": "cd ws-server && npm run start",
    "format": "prettier . --write",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^6.2.0",
    "@mui/material": "^6.2.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.7",
    "chartjs-plugin-datalabels": "^2.2.0",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.7",
    "flowbite-react": "^0.8.0",
    "formdata-node": "^6.0.3",
    "formidable": "^3.5.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "next": "14.1.3",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.10",
    "@types/react": "^18",
    "@types/react-chartjs-2": "^2.5.7",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.2",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "form-data": "^4.0.1",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
